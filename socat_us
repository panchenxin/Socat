#!/bin/bash

#IPv4 forward
/usr/bin/socat TCP4-LISTEN:55555,fork TCP4:23.166.24.111:55555 &
/usr/bin/socat UDP4-LISTEN:55555,fork UDP4:23.166.24.111:55555 &



#IPv6 forward
#/usr/bin/socat TCP4-LISTEN:443,fork TCP6:[2606:4700:4700::1111]:80 &
#/usr/bin/socat UDP4-LISTEN:443,fork UDP6:[2606:4700:4700::1001]:80 &

wait
